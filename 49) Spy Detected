#include <bits/stdc++.h>
using namespace std;
int main(){
    int t; cin>>t;
    while(t--){
        int n; cin>>n;
        map<int,int> mp;
        vector<int>a(n);
        for(int &x:a){cin>>x; mp[x]++;}
        for(int i=0;i<n;i++)
            if(mp[a[i]]==1){
                cout<<i+1<<"\n"; break;
            }
    }
}
