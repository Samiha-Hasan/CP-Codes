#include <bits/stdc++.h>
using namespace std;
int main(){
    int t; cin>>t;
    while(t--){
        int n; cin>>n;
        string s; cin>>s;
        map<char,int> mp;
        bool ok=true;
        for(int i=0;i<n;i++){
            if(i>0 && s[i]!=s[i-1] && mp[s[i]]) ok=false;
            mp[s[i]]++;
        }
        cout<<(ok?"YES\n":"NO\n");
    }
}
