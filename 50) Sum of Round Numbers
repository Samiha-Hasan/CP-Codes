#include <bits/stdc++.h>
using namespace std;
int main(){
    int t; cin>>t;
    while(t--){
        int n; cin>>n;
        vector<int>v;
        int p=1;
        while(n){
            if(n%10) v.push_back((n%10)*p);
            n/=10; p*=10;
        }
        cout<<v.size()<<"\n";
        for(int x:v) cout<<x<<" ";
        cout<<"\n";
    }
}
