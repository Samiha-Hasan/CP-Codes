#include <bits/stdc++.h>
using namespace std;

int main(){
    int t; cin >> t;
    while(t--){
        int n; cin >> n;
        vector<pair<int,int>> a(n), b(n);
        for(int i=0;i<n;i++){
            cin >> a[i].first;
            a[i].second = i;
        }
        for(int i=0;i<n;i++){
            cin >> b[i].first;
        }
        sort(a.begin(), a.end());
        sort(b.begin(), b.end());
        vector<int> ans(n);
        for(int i=0;i<n;i++){
            ans[a[i].second] = b[i].first;
        }
        for(int x:ans) cout << x << " ";
        cout << "\n";
    }
}
